&time_settings
run_days         = 12
run_hours        = 0
run_minutes      = 0
run_seconds      = 0
dt               = 120
history_interval = 120
integral_scheme  = 'RK3_TVD' ! Choose from 'RK3_TVD' or 'RK4'
/

&case_select
case_num    = 2, ! Choose from 2, 5 or 6
/

&domain
dx                   = 3,  ! Unit: degree 
dy                   = 3,  ! Unit: degree
xhalo                = 2,  ! The ghost cell size
yhalo                = 2,  ! The ghost cell size
nPointsOnEdge        = 3,
nQuadOrder           = 3,  ! Do not change for now
/

&dynamic_opt
reconstruct_scheme = 'WENO'
stencil_width      =  5,
/
