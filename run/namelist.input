&time_settings
run_days         = 15
run_hours        = 0
run_minutes      = 0
run_seconds      = 0
dt               = 600 ! 300s for 1 degree
history_interval = 86400
integral_scheme  = 'RK3_WRF' ! Choose from 'RK3_TVD' or 'RK3_WRF' or 'RK4' or 'PC2' or 'SSPRK'
/

&case_select
case_num    = 5, ! Choose from 2, 5, 6, 8, 9
/

&domain
dx                   = 2,  ! Unit: degree
dy                   = 2,  ! Unit: degree
nPointsOnEdge        = 3,
/

&dynamic_opt
reconstruct_scheme         = 'WENO' ! Choose from 'Polynomial', 'WENO3', 'WENO5', 'WENO'
epsilon                    = 1.e-2! Coefficient for avioding divide 0 in WLS-ENO
stencil_width              = 5,
quad_opt                   = 1, ! 1 for square gaussian quadratrue, 2 for triangular quaduature
nQuadOrder                 = 4,
use_trouble_cell_indicator = .true.
/
